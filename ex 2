// додайте змінну "firstName" та "lastName"
//таким чином, щоб вийшло Ваше повне ім'я
let firstName = "Olga";
let lastName = "Zubar";

// створіть змінну, у якій збергіається відповідь (повне ім'я)
// для "firstName" + " " + "lastName"
let firstName = "Olga";
let lastName = " Zubar";
let fullName = firstName + lastName;
console.log(fullName);



